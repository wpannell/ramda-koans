{
  "name": "ramda-koans",
  "version": "1.0.0",
  "description": "Koans for ramda.js inspired by the Ruby Koans",
  "main": "test-helper.js",
  "scripts": {
    "lint": "esw -f simple-detail spec",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --compilers js:babel-core/register --require ./test-helper.js --growl --recursive spec",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpannell/ramda-koans.git"
  },
  "keywords": [
    "ramda"
  ],
  "author": "Wil Pannell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wpannell/ramda-koans/issues"
  },
  "homepage": "https://github.com/wpannell/ramda-koans#readme",
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "chai-immutable": "1.5.4",
    "eslint": "2.13.0",
    "eslint-plugin-mocha": "3.0.0",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.11",
    "growl": "1.9.2",
    "growly": "1.3.0",
    "mocha": "2.4.5",
    "should": "8.4.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "immutable": "3.8.1",
    "q": "1.4.1",
    "ramda": "0.21.0",
    "ramda-cli": "1.2.2"
  },
  "engines": {
    "node": "6.2.2"
  }
